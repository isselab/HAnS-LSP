{
  "name": "lsp-hans-vscode-plugin",
  "displayName": "lsp_HAnS_VSCode_plugin",
  "description": "plugin for LSP_HAnS together with VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "Java",
        "extensions": [
          ".java"
        ]
      },
      {
        "id": "C",
        "extensions": [
          ".c"
        ]
      },
      {
        "id": "javascript",
        "extensions": [
          ".js"
        ]
      },
      {
        "id": "typescript",
        "extensions": [
          ".ts"
        ]
      },
      {
        "id": ".Feature-Model",
        "extensions": [
          ".Feature-Model"
        ]
      },
      {
        "id": ".Feature-To-File",
        "extensions": [
          ".Feature-To-File"
        ]
      }
    ],
    
    "commands": [{
      "command": "lsp-hans-vscode-plugin.helloWorld",
      "title": "Hello World"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "eslint": "^9.11.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
