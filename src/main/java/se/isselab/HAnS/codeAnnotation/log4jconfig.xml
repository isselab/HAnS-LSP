<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <Appenders>
    <File name="Log" fileName="log.log">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </File>
  </Appenders>
  <Loggers>
    <Root level="trace">
      <AppenderRef ref="Log"/>
    </Root>
  </Loggers>
</Configuration>