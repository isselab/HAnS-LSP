{
    "scopeName": "HAnS-vscode.injection", 
    "injectionSelector": ["L:comment.line.double-slash.java"],
    "patterns": [{ "include": "#expression" }],
    "repository": {
      "expression": {
        "patterns": [{"include" : "#text"},{"include": "#Annotation"},{"include": "#text"}]
      },   
      "Annotation" : {
        "patterns": [{"include":"#Annotationtype"},{"include":"#features"}]
      },
      "text" : {
        "patterns": [{"match" : "(.)*"  }]
      },
      "features" : {
        "begin": "[",
        "end":"]",
        "beginCaptures": {
            "0": { "name": "annotation.features.open" }
          },
          "endCaptures": {
            "0": { "name": "annotation.features.close" }
          },
        "name": "annotation.features",
        "patterns": [{"match" : "([0-9]|[a-z]|[A-Z]|_|:|,)*"  }]
      },
      "Annotationtype" : {
        "name": "annotation.type" ,
        "patterns": [{"match" : "(&begin|&Begin|&End|&end|&Line|&line)"  }]
      },
      "featurename" : {
        "name": "annotation.feature" ,
        "patterns": [{"match" : "(&begin|&Begin|&End|&end|&Line|&line)"  }]
      }
    }
  }